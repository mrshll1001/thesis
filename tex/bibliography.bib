	pages = {673--682},
	file = {ScienceDirect Full Text PDF:/home/matt/Zotero/storage/GPUD6BCJ/Glassman and Kang - 2012 - Intelligence in the internet age The emergence an.pdf:application/pdf;ScienceDirect Full Text PDF:/home/matt/Zotero/storage/TKVFGEID/Glassman and Kang - 2012 - Intelligence in the internet age The emergence an.pdf:application/pdf;ScienceDirect Snapshot:/home/matt/Zotero/storage/HCV5A9F5/S0747563211002585.html:text/html;ScienceDirect Snapshot:/home/matt/Zotero/storage/UNQ334EU/S0747563211002585.html:text/html}

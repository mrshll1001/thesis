	pages = {5015--5027},
	file = {ACM Full Text PDF:/home/matt/Zotero/storage/UH2UETCB/Elsden et al. - 2016 - ResViz Politics and Design Issues in Visualizing .pdf:application/pdf}
}

@inproceedings{elsden_metadating:_2016,
